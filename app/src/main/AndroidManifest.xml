<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finish1m">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Finish1M">
        <activity
            android:name=".Presentation.CreateNewEventActivity"
            android:exported="false" />
        <activity
            android:name=".Presentation.ChatSettingsActivity"
            android:exported="false" />
        <activity
            android:name=".Presentation.ChatActivity"
            android:exported="false" />
        <activity
            android:name=".Presentation.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".Presentation.HubActivityActivity"
            android:exported="false"
            android:label="@string/title_activity_hub_activity"
            android:theme="@style/Theme.Finish1M.NoActionBar" />
        <activity
            android:name=".Presentation.EnterActivity"
            android:exported="false" />
        <activity
            android:name=".Presentation.StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity android:name=".Presentation.RegisterActivity" />
    </application>

</manifest>